-- Other --

local erijnierniwcuiwhugr = ot:CreateLabel("Anti Sit Function")

local weijbrhberinwuiwebfweg = ot:CreateToggle("Anti Sit V1", "", function(wefn)
   wridasd = wefn
   while wridasd == true do
      if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
         task.wait()
         game.Players.LocalPlayer.Character.Humanoid.Sit = false
      end
task.wait()
   end
end)

local weijbrhberinwuiwebfweg = ot:CreateToggle("Anti Sit V2", "", function(ergergsdacs)
   antisit = ergergsdacs
   if antisit == false then 
      for sitpart,allsitpart in pairs(game.workspace:GetDescendants()) do
          if allsitpart:IsA("Seat") or allsitpart:IsA("VehicleSeat") then
              if allsitpart.Disabled == true then
                  allsitpart.Disabled = false
              end
          end
      end
      return 
   elseif antisit == true then
      for sitpart,allsitpart in pairs(game.workspace:GetDescendants()) do
          if allsitpart:IsA("Seat") or allsitpart:IsA("VehicleSeat") then
              if allsitpart.Disabled == false then
               if disantisitca == true then
                  if allsitpart.Name == "Passenger" == false or allsitpart.Name == "PassengerBaby" == false or allsitpart.Name == "VehicleSeat" == false then
                     if disantisittol == true then  
                        if allsitpart.Parent.Name == "ShoppingCart" or allsitpart.Parent.Name == "Wagon" or allsitpart.Parent.Name == "Stretcher" or allsitpart.Parent.Name == "LawnMower"  or allsitpart.Parent.Name == "Couch" then
                           allsitpart = false
                        end
                     end
                  allsitpart.Disabled = true
               end
              end
          end
      end
   end
   local function asdawdsccxxc(allsitparttt)
       for sitpart,allsitpart in pairs(game.workspace:GetDescendants()) do
           if allsitpart:IsA("Seat") or allsitpart:IsA("VehicleSeat") then
               if allsitpart.Disabled == false then
                if disantisitca == true then
                   if allsitpart.Name == "Passenger" == false or allsitpart.Name == "PassengerBaby" == false or allsitpart.Name == "VehicleSeat" == false then
                      if disantisittol == true then  
                         if allsitpart.Parent.Name == "ShoppingCart" or allsitpart.Parent.Name == "Wagon" or allsitpart.Parent.Name == "Stretcher" or allsitpart.Parent.Name == "LawnMower"  or allsitpart.Parent.Name == "Couch" then
                            allsitpart = false
                         end
                      end
                   allsitpart.Disabled = true
                end
               end
           end
       end
    end
   end
   end
  game:GetService("Workspace").DescendantAdded:Connect(
      function(allsitparttt)
          asdawdsccxxc(allsitparttt)
      end)
end)

local weijbrhberinwuiwebfweg = ot:CreateToggle("Anti Sit V3", "", function(wefn)
   wridasd = wefn
   while wridasd == true do
      if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
         task.wait()
         game.Players.LocalPlayer.Character.Humanoid.Jump = true
      end
task.wait()
   end
end)

