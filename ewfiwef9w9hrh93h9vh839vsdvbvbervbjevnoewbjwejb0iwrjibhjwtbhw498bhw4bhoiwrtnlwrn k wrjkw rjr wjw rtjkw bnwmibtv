local jobidplaceid = serva:CreateButton("Create Join Server Script", "",
function()
setclipboard( [[game:GetService("TeleportService"):TeleportToPlaceInstance(]]..game.PlaceId..", "..game.JobId..", "..[[game:GetService("Players").LocalPlayer)]] )
game:GetService("StarterGui"):SetCore("SendNotification",{Title="Script",Text="Created, And Copied!",Duration=0.1})
end)

local iewfwoefnwjcw = serva:CreateLabel("Other Server")

local yuyumymujwdwvwv = serva:CreateButton("Change Server / Server HOP", "",
function()
    game:GetService("StarterGui"):SetCore("SendNotification",{Title="Script",Text="Hoping..",Duration=0.3})
    local Player = game.Players.LocalPlayer    
    local Http = game:GetService("HttpService")
    local TPS = game:GetService("TeleportService")
    local Api = "https://games.roblox.com/v1/games/"
    
    local _place,_id = game.PlaceId, game.JobId
    local _servers = Api.._place.."/servers/Public?sortOrder=Desc&limit=100"
    function ListServers(cursor)
       local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
       return Http:JSONDecode(Raw)
    end
    
    local Next; repeat
       local Servers = ListServers(Next)
       for i,v in next, Servers.data do
           if v.playing < v.maxPlayers and v.id ~= _id then
            game:GetService("StarterGui"):SetCore("SendNotification",{Title="Script",Text="Found! And Joining..",Duration=0.3})
            wait()
               local s,r = pcall(TPS.TeleportToPlaceInstance,TPS,_place,v.id,Player)
               if s then break end
           end
       end
       
       Next = Servers.nextPageCursor
    until not Next
end)

local wiefnwnkcss = serva:CreateButton("Join Lowest Players Server", "",
function()
    game:GetService("StarterGui"):SetCore("SendNotification",{Title="Script",Text="Finding..",Duration=0.3})
    local Http = game:GetService("HttpService")
    local TPS = game:GetService("TeleportService")
    local Api = "https://games.roblox.com/v1/games/"
    
    local _place = game.PlaceId
    local _servers = Api.._place.."/servers/Public?sortOrder=Asc&limit=100"
    function ListServers(cursor)
       local Raw = game:HttpGet(_servers .. ((cursor and "&cursor="..cursor) or ""))
       return Http:JSONDecode(Raw)
    end
    
    local Server, Next; repeat
       local Servers = ListServers(Next)
       Server = Servers.data[1]
       Next = Servers.nextPageCursor
    until Server
    game:GetService("StarterGui"):SetCore("SendNotification",{Title="Script",Text="Found! And Joining..",Duration=0.3})
    wait()
    TPS:TeleportToPlaceInstance(_place,Server.id,game.Players.LocalPlayer)
end)
CreateNotification("Loaded", "Script is now loaded, Enjoy using our script!", function() end)
wait(1)
CreateNotification("Discord Invite", "Copy discord invite or no?", function() setclipboard("https://discord.gg/pmpnjtbrA3") end)
